{
  "name": "tboon",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx,css}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,mdx,css}\"",
    "types": "tsc --noEmit",
    "typecheck": "tsc --noEmit --incremental false",
    "clean": "trash .next && trash .next/cache && trash node_modules && rm -rf pnpm-lock.yaml",
    "clean:build": "trash .next",
    "clean:cache": "trash .next/cache",
    "reboot": "pnpm clean && pnpm install"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-query-devtools": "^5.66.0",
    "antd": "^5.24.0",
    "classnames": "^2.5.1",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.475.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-input-mask": "^2.0.4",
    "react-phone-input-2": "^2.15.1",
    "react-router-dom": "^7.1.5"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.6",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@reduxjs/toolkit": "^2.5.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/eslint-plugin-security": "^3.0.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.13.2",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/react-input-mask": "^3.0.6",
    "@types/redux-logger": "^3.0.13",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.9",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.0",
    "react-redux": "^9.2.0",
    "redux-logger": "^3.0.6",
    "sass": "^1.84.0",
    "trash": "^9.0.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=7.0.0"
